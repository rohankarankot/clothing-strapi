{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "sku": {
      "type": "string"
    },
    "Price": {
      "displayName": "price",
      "type": "component",
      "repeatable": false,
      "component": "product.price"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "newarrival": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "unsex"
      ]
    },
    "configurableOptions": {
      "displayName": "Configurable Options",
      "type": "component",
      "repeatable": true,
      "component": "product.configurable-options"
    },
    "vendor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vendor.vendor"
    }
  }
}
